<Window x:Class="AnalyzerObfuscator.ObfuscationResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnalyzerObfuscator"
        mc:Ignorable="d"
        Title="ObfuscationResultWindow" Height="450" Width="800">
    <Grid>
        <FlowDocumentReader x:Name="reader" Margin="10,35,10,10">
        </FlowDocumentReader>
        <!--<TextBlock x:Name="textBlock" TextWrapping="Wrap" Margin="10,35,10,10"/>-->
        <Button x:Name="save" Content="Save" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="save_Click"/>

    </Grid>
</Window>
